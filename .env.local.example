# SOVR Empire Environment Variables
# Copy this to .env.local and fill in your values

# Supabase (get from https://supabase.com)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Wallet Connect (get from https://walletconnect.com)
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id

# Coinbase (get from https://developers.coinbase.com)
NEXT_PUBLIC_COINBASE_APP_ID=your_coinbase_app_id

# Smart Contract Addresses (deploy your own or use testnet)
NEXT_PUBLIC_SOVR_CONTRACT_ADDRESS=0x742d35Cc6634C0532925a3b8D4C0532925a3b8D4
NEXT_PUBLIC_ECHO_CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890
NEXT_PUBLIC_VAULT_CONTRACT_ADDRESS=0x742d35Cc6634C0532925a3b8D4C0532925a3b8D4

# Network Configuration
NEXT_PUBLIC_NETWORK_NAME=polygon
NEXT_PUBLIC_CHAIN_ID=137

# App Configuration
NEXT_PUBLIC_APP_NAME=SOVR Empire
NEXT_PUBLIC_APP_VERSION=1.0.0
